<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dz.member.dao.MemberDAO">
    <select id="list" resultType="com.dz.member.vo.MemberVO">
		select
        	 *
        from member
   </select>
    <select id="findById" resultType="com.dz.member.vo.MemberLoginParam">
        select
            *
        from member
        where userId = #{userId}
    </select>
    <insert id="save">
        insert into member(
            userId, pwd
        ) values (
                  #{userId}, #{pwd}
                         )

    </insert>
</mapper>